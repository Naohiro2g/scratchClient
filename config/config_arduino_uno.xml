<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config version="1.0">
    <description>Generated configuration from ScratchClientConfig</description>
    <adapter class="adapter.arduino.UNO_Adapter" name="UNO">
        
        <!-- id = 'D2'  direction = 'out'  function = 'output' -->
        <input_value name="inputD2">
            <variable name="a"/>
        </input_value>
        
        <!-- id = 'D3'  direction = 'in'  function = 'input_pullup' -->
        <output_value name="outputD3">
            <sensor name="button_3"/>
        </output_value>
        
        <!-- id = 'D4'  direction = 'out'  function = 'output' -->
        <input_value name="inputD4">
            <variable name="redLED"/>
        </input_value>
        
        <!-- id = 'D5'  direction = 'out'  function = 'pwm' -->
        <input_value name="pwmD5">
            <variable name="greenLED_PWM"/>
        </input_value>
        
        <!-- id = 'D6'  direction = 'out'  function = 'servo' -->
        <input_value name="servoD6">
            <variable name="servoD6"/>
        </input_value>
        
        <!-- id = 'D7'  direction = 'out'  function = 'output' -->
        <input_value name="inputD7">
            <variable name="button LED"/>
        </input_value>
        
        <!-- id = 'D8'  direction = 'in'  function = 'input_pullup' -->
        <output_value name="outputD8">
            <sensor name="b"/>
        </output_value>
        
        <!-- id = 'D9'  direction = 'in'  function = 'input_pullup' -->
        <output_value name="outputD9">
            <sensor name="c"/>
        </output_value>
        
        <!-- id = 'D10'  direction = 'in'  function = 'input_pullup' -->
        <output_value name="outputD10">
            <sensor name="d"/>
        </output_value>
        
        <!-- id = 'D12'  direction = 'in'  function = 'input_pullup' -->
        <output_value name="outputD12">
            <sensor name=""/>
        </output_value>
        
        <!-- id = 'A0'  direction = 'in'  function = 'analog' -->
        <output_value name="outputA0">
            <sensor name="potentiometer_0"/>
        </output_value>
        
        <extension>
            <io dir="out" id="D2"/>
            <io dir="in" id="D3" pullup="on"/>
            <io dir="out" id="D4"/>
            <io dir="pwm" id="D5"/>
            <io dir="servo" id="D6"/>
            <io dir="out" id="D7"/>
            <io dir="in" id="D8" pullup="on"/>
            <io dir="in" id="D9" pullup="on"/>
            <io dir="in" id="D10" pullup="on"/>
            <io dir="in" id="D12" pullup="on"/>
            <analog dir="analog_in" id="A0"/>
        </extension>
        
        <parameter name="serial.device" value="/dev/ttyUSB0"/>
        <parameter name="serial.baud" value="115200"/>
        
        <!-- optional parameters for IDENT check -->
        <parameter name="ident.check" value="no"/>
        <parameter name="ident.pattern" value=".*"/>
    </adapter>
</config>
